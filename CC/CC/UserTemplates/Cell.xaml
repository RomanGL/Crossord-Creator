<UserControl x:Class="Crossword_Application_Modern.UserTemplates.Cell"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="60" d:DesignWidth="71" Width="25" Height="25">    
    <Grid>
        <Rectangle x:Name="rect" Fill="{DynamicResource ButtonBackground}" 
                   Stroke="{DynamicResource ButtonBorderPressed}"
                   Height="25" Width="25">            
        </Rectangle>
        <TextBlock x:Name="symbolTextBlock" Text=" " TextAlignment="Center" FontSize="19"
                   Height="25" Width="25" 
                   ScrollViewer.VerticalScrollBarVisibility="Disabled" Foreground="{DynamicResource WindowText}"/>        
    </Grid>
</UserControl>
